{
  "name": "proyect-daw-ii",
  "version": "0.0.0",
  "scripts": {
    "pug": "pug -O \"{doctype: 'html'}\" src",
    "pug:w": "pug -O \"{doctype: 'html'}\" src -P -w",
    "tsc": "tsc *.ts utils/*.ts -t es5 --removeComments",
    "build": "npm run pug && ng build --prod --aot --output-hashing=none",
    "postinstall": "npm run build",
    "start": "node server.js",
    "serve": "npm-run-all -p pug:w serve:open",
    "serve:open": "ng serve --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "bootstrap": "^5.1.3",
    "g": "^2.0.1",
    "ngx-toastr": "^14.2.1",
    "primeicons": "^4.1.0",
    "primeng": "^12.1.1",
    "pug": "^3.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "^12.2.12",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/jquery": "^3.5.8",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.3.5"
  }
}
